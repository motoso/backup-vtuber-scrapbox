      "created": 1525193412,
      "updated": 1525193412
    },
    {
      "id": "5ae89ac4774b17000060358e",
      "text": "自己紹介動画",
      "userId": "59fb21121207900012774b17",
      "created": 1525193412,
      "updated": 1525193412
    },
    {
      "id": "5ae89ac4774b17000060358f",
      "text": " [https://www.youtube.com/watch?v=SFpLbMuZEyk]",
      "userId": "59fb21121207900012774b17",
