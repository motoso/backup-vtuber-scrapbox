    {
      "id": "5abb57d7774b1700004fbe9a",
      "text": "要約",
      "userId": "59fb21121207900012774b17",
      "created": 1522227159,
      "updated": 1522227159
    },
    {
      "id": "5abb95f9774b1700000d24c5",
      "text": " [株式会社シテイル]代表取締役社長",
      "userId": "59fb21121207900012774b17",
      "created": 1522243066,
      "updated": 1522243308
    },
    {
      "id": "5abb57d7774b1700004fbe9b",
      "text": " 採用活動として会社の広報を動画にしている",
      "userId": "59fb21121207900012774b17",
      "created": 1522227159,
      "updated": 1522243155
    },
    {
      "id": "5abb9654774b1700000d24c8",
      "text": "  どういう雰囲気で仕事をしているのかを伝えるため",
      "userId": "59fb21121207900012774b17",
      "created": 1522243158,
      "updated": 1522243171
    },
    {
