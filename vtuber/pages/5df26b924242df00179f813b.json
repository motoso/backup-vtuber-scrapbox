      "updated": 1576168343
    },
    {
      "id": "5df26b96774b170000f01395",
      "text": "特記事項",
      "userId": "59fb21121207900012774b17",
      "created": 1576168343,
      "updated": 1576168343
    },
    {
      "id": "5df26b96774b170000f01396",
      "text": "　",
      "userId": "59fb21121207900012774b17",
      "created": 1576168343,
      "updated": 1576168343
    },
    {
      "id": "5df26b96774b170000f01397",
