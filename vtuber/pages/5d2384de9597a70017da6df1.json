      "created": 1562608873,
      "updated": 1562608873
    },
    {
      "id": "5d2384e9774b1700004301f1",
      "text": "特記事項",
      "userId": "59fb21121207900012774b17",
      "created": 1562608873,
      "updated": 1562608873
    },
    {
      "id": "5d2384e9774b1700004301f2",
      "text": "",
