      "updated": 1562608868
    },
    {
      "id": "5d2384e4774b170000754e09",
      "text": "特記事項",
      "userId": "59fb21121207900012774b17",
      "created": 1562608868,
      "updated": 1562608868
    },
    {
      "id": "5d2384e4774b170000754e0a",
